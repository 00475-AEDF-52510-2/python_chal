   "source": [
    "def fact(a):\n",
    "    if a == 0 or a == 1:\n",
    "        return 1    # 조기리턴\n",
    "    return a * fact(a - 1)\n",
    "\n",
    "print(fact(5))\n",
    "print(fact(32))"
   ]
  }
